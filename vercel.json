{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    { "source": "/api/:path*", "destination": "/api/:path*" },
    { "source": "/app/history/public/workout/(.*)", "destination": "/api/workout/workoutId?id=$1" },
    { "source": "/app/history/public/(.*)", "destination": "/api/history/userId?id=$1" },
    { "source": "/history/public/workout/(.*)", "destination": "/api/workout/workoutId?id=$1" },
    { "source": "/history/public/(.*)", "destination": "/api/history/userId?id=$1" },
    { "source": "/app/routines/public/(.*)", "destination": "/api/routines/routineId?id=$1" },
    { "source": "/routines/public/(.*)", "destination": "/api/routines/routineId?id=$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
} 